{
  "name": "ReactReduxBoilerplate",
  "version": "1.0.0",
  "description": "A React + Redux boilerplate powered by Webpack. Stylesheets with less. Testing with Mocha and Chai.",
  "main": "server.js",
  "scripts": {
    "build:webpack:devserver": "NODE_ENV=production webpack --progress --config webpack/webpack.config.devserver.js",
    "build:webpack:integration": "NODE_ENV=integration webpack --progress --config webpack/webpack.config.integration.js",
    "build:webpack:pilot": "NODE_ENV=pilot webpack --progress --config webpack/webpack.config.pilot.js",
    "build:webpack:prod": "NODE_ENV=prod webpack --progress --config webpack/webpack.config.prod.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "start": "node server.js",
    "test": "karma start karma/karma.config.development.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "flux",
    "todomvc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiandeutsch/react-redux-starter.git"
  },
  "license": "MIT",
